{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "1.0",
  "description": "__MSG_appDesc__",
  "author": "Matthew de Marillac",
  "default_locale": "en",
  "icons": {
      "16": "icons/16.png",
      "19": "icons/19.png",
      "32": "icons/32.png",
      "38": "icons/76.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
   },
  "permissions": ["activeTab"],
  "page_action": {
    "default_title": "__MSG_appName__"
    },
    "content_scripts": [
    {
      "matches": ["*://*.kissanime.to/*", "*://*.kisscartoon.me/*", "*://*.kissasian.com/*"],
      "js": ["jquery-1.7.1.min.js", "AutoPlay.js"]
    }
  ],
  "background": {
  "matches": [
        "*://*.kissanime.to/*", "*://*.kisscartoon.me/*", "*://*.kissasian.com/*"
    ],
    "scripts": ["bootstrap.js"],
    "persistent": false
  }
}
